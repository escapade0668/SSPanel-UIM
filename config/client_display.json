{
  "clients": [
    {
      "name": "Clash Verge Rev",
      "description": "功能完善的图形化客户端",
      "format": "clash",
      "importUrl": "clash://install-config?url={sub}/clash",
      "platforms": {
        "Windows": {
          "ext": "exe",
          "file": "Clash.Verge"
        },
        "macOS": {
          "ext": "dmg",
          "file": "Clash.Verge"
        },
        "Linux": {
          "ext": "deb",
          "file": "Clash.Verge"
        }
      }
    },
    {
      "name": "Clash Nyanpasu",
      "description": "增强版 Clash 客户端",
      "format": "clash",
      "importUrl": "clash-nyanpasu://subscribe-remote-profile?url={sub}/clash&name={name}",
      "platforms": {
        "Windows": {
          "ext": "exe",
          "file": "Clash.Nyanpasu"
        },
        "macOS": {
          "ext": "dmg",
          "file": "Clash.Nyanpasu_aarch64"
        },
        "Linux": {
          "ext": "AppImage",
          "file": "Clash.Nyanpasu"
        }
      }
    },
    {
      "name": "CMFA",
      "description": "支持分应用代理的 Android 客户端",
      "format": "clash",
      "importUrl": "clash://install-config?url={sub}/clash&name={name}",
      "platforms": {
        "Android": {
          "ext": "apk",
          "file": "CMFA"
        }
      }
    },
    {
      "name": "ClashMi",
      "description": "基于 Mihomo 内核的新客户端",
      "format": "clash",
      "importUrl": "clash://install-config?url={sub}/clash&name={name}",
      "platforms": {
        "Windows": {
          "ext": "exe",
          "file": "clashmi"
        },
        "Android": {
          "ext": "apk",
          "file": "clashmi_arm64"
        },
        "iOS": {
          "storeUrl": "https://apps.apple.com/us/app/clash-mi/id6744321968"
        },
        "macOS": {
          "storeUrl": "https://apps.apple.com/us/app/clash-mi/id6744321968"
        }
      }
    }
  ],
  "icons": {
    "Windows": "ti-brand-windows",
    "macOS": "ti-brand-finder",
    "Android": "ti-brand-android",
    "iOS": "ti-brand-apple",
    "Linux": "ti-brand-ubuntu"
  }
}
